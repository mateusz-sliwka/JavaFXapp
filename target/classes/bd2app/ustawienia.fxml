<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bd2app.UstawieniaController">
    <children>
        <Button fx:id="logoutButton" layoutX="532.0" layoutY="8.0" mnemonicParsing="false" onAction="#logout" text="Wyloguj" />
        <Button fx:id="myAccountButton" layoutX="445.0" layoutY="8.0" mnemonicParsing="false" onAction="#showMyAccount" text="Moje konto" />
        <Button fx:id="returnButton" layoutX="7.0" layoutY="8.0" mnemonicParsing="false" onAction="#goBack" text="Powrót" />
      <Button fx:id="newSettingButton" layoutX="433.0" layoutY="369.0" mnemonicParsing="false" onAction="#newTransaction" text="Dodaj" />
      <TableView fx:id="settingsList" layoutY="40.0" prefHeight="323.0" prefWidth="604.0">
        <columns>
          <TableColumn fx:id="name" prefWidth="300.0" text="Nazwa" />
          <TableColumn fx:id="value" prefWidth="300.0" text="Wartość" />
        </columns>
      </TableView>
      <Button fx:id="editSettingButton" layoutX="492.0" layoutY="369.0" mnemonicParsing="false" onAction="#editTransaction" text="Edytuj" />
      <Button fx:id="deleteButton" layoutX="549.0" layoutY="369.0" mnemonicParsing="false" onAction="#deleteSetting" text="Usuń" />
    </children>
</AnchorPane>
