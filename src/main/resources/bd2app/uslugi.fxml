<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bd2app.UslugiController">
   <children>
      <Button fx:id="logoutButton" layoutX="532.0" layoutY="8.0" mnemonicParsing="false" onAction="#logout" text="Wyloguj" />
      <Button fx:id="myAccountButton" layoutX="445.0" layoutY="8.0" mnemonicParsing="false" onAction="#showMyAccount" text="Moje konto" />
      <Button fx:id="returnButton" layoutX="7.0" layoutY="8.0" mnemonicParsing="false" onAction="#goBack" text="Powrót" />
      <TableView fx:id="servicesList" layoutY="40.0" prefHeight="320.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="name" prefWidth="196.0" text="Nazwa" />
          <TableColumn fx:id="price" prefWidth="207.0" text="Cena" />
            <TableColumn fx:id="category" prefWidth="196.0" text="Kategoria" />
        </columns>
      </TableView>
      <Button fx:id="addServiceButton" layoutX="429.0" layoutY="367.0" mnemonicParsing="false" onAction="#addService" text="Dodaj" />
      <Button fx:id="deleteServiceButton" layoutX="547.0" layoutY="367.0" mnemonicParsing="false" onAction="#deleteService" text="Usuń" />
      <Button fx:id="editServiceButton" layoutX="486.0" layoutY="367.0" mnemonicParsing="false" onAction="#editService" text="Edytuj" />
   </children>
</AnchorPane>
